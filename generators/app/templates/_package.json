{
	"name": "<%= themeDirName %>",
	"version": "<%= packageVersion %>",
	"keywords": [
		"liferay-theme"
	],
	"liferayTheme": {
		"baseTheme": "styled",
		"screenshot": "",
		"supportCompass": <%= supportCompass %>,
		"templateLanguage": "<%= templateLanguage %>",
		"version": "<%= liferayVersion %>"
	},
	"dependencies": {
		"gulp": "^3.8.10",
		"liferay-theme-tasks": "*"
	}<% if (publishTag) { %>,
	"publishConfig": {
		"tag": "<%= publishTag %>"
	}<% } %>
}